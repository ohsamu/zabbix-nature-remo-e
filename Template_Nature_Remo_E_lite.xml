<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2020-07-26T07:54:05Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Nature Remo E lite</template>
            <name>Template Nature Remo E lite</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <discovery_rules>
                <discovery_rule>
                    <name>remo-discovery</name>
                    <type>HTTP_AGENT</type>
                    <key>remo-discovery</key>
                    <delay>1d</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>measured_instantaneous[{#DEVICE_NAME}]</name>
                            <type>DEPENDENT</type>
                            <key>measured_instantaneous[{#DEVICE_NAME}]</key>
                            <delay>0</delay>
                            <history>1200d</history>
                            <trends>1200d</trends>
                            <units>W</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[?(@.id == '{#APPLIANCE_ID}')].smart_meter.echonetlite_properties[?(@.name == 'measured_instantaneous')].val.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>remo-json[{#DEVICE_NAME}]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=2000</expression>
                                    <name>Over 2kW</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>normal_direction_cumulative_electric_energy[{#DEVICE_NAME}]</name>
                            <type>HTTP_AGENT</type>
                            <key>normal_direction_cumulative_electric_energy[{#DEVICE_NAME}]</key>
                            <delay>0;m0,30</delay>
                            <history>1200d</history>
                            <trends>1200d</trends>
                            <value_type>FLOAT</value_type>
                            <units>kWh</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[?(@.id == '{#APPLIANCE_ID}')].smart_meter.echonetlite_properties[?(@.name == 'normal_direction_cumulative_electric_energy')].val.first()</params>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.1</params>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <params/>
                                </step>
                            </preprocessing>
                            <timeout>10s</timeout>
                            <url>https://api.nature.global/1/appliances</url>
                            <headers>
                                <header>
                                    <name>Authorization</name>
                                    <value>Bearer {$API_KEY}</value>
                                </header>
                            </headers>
                        </item_prototype>
                        <item_prototype>
                            <name>remo-json[{#DEVICE_NAME}]</name>
                            <type>HTTP_AGENT</type>
                            <key>remo-json[{#DEVICE_NAME}]</key>
                            <history>10d</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <timeout>10s</timeout>
                            <url>https://api.nature.global/1/appliances</url>
                            <headers>
                                <header>
                                    <name>Authorization</name>
                                    <value>Bearer {$API_KEY}</value>
                                </header>
                            </headers>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>measured_instantaneous[{#DEVICE_NAME}]</name>
                            <graph_items>
                                <graph_item>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>0080FF</color>
                                    <item>
                                        <host>Template Nature Remo E lite</host>
                                        <key>measured_instantaneous[{#DEVICE_NAME}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>FF8000</color>
                                    <calc_fnc>MAX</calc_fnc>
                                    <item>
                                        <host>Template Nature Remo E lite</host>
                                        <key>measured_instantaneous[{#DEVICE_NAME}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>normal_direction_cumulative_electric_energy[{#DEVICE_NAME}]</name>
                            <graph_items>
                                <graph_item>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>FF9900</color>
                                    <item>
                                        <host>Template Nature Remo E lite</host>
                                        <key>normal_direction_cumulative_electric_energy[{#DEVICE_NAME}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>FF4000</color>
                                    <calc_fnc>MAX</calc_fnc>
                                    <item>
                                        <host>Template Nature Remo E lite</host>
                                        <key>normal_direction_cumulative_electric_energy[{#DEVICE_NAME}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                    <url>https://api.nature.global/1/appliances</url>
                    <headers>
                        <header>
                            <name>Authorization</name>
                            <value>Bearer {$API_KEY}</value>
                        </header>
                    </headers>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#APPLIANCE_ID}</lld_macro>
                            <path>$.id</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#DEVICE_NAME}</lld_macro>
                            <path>$.device.name</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.[?(@.type == 'EL_SMART_METER')]</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$API_KEY}</macro>
                </macro>
            </macros>
            <screens>
                <screen>
                    <name>Nature Remo E Graphs</name>
                    <hsize>1</hsize>
                    <vsize>2</vsize>
                    <screen_items>
                        <screen_item>
                            <resourcetype>20</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>measured_instantaneous[{#DEVICE_NAME}]</name>
                                <host>Template Nature Remo E lite</host>
                            </resource>
                            <width>900</width>
                            <height>200</height>
                            <x>0</x>
                            <y>0</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>1</max_columns>
                        </screen_item>
                        <screen_item>
                            <resourcetype>20</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>normal_direction_cumulative_electric_energy[{#DEVICE_NAME}]</name>
                                <host>Template Nature Remo E lite</host>
                            </resource>
                            <width>900</width>
                            <height>200</height>
                            <x>0</x>
                            <y>1</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>1</max_columns>
                        </screen_item>
                    </screen_items>
                </screen>
            </screens>
        </template>
    </templates>
</zabbix_export>
